{
  "format_version": "0.1",
  "terraform_version": "0.14.0",
  "configuration": {
    "provider_config": {
      "aws": {
        "expressions": {
          "region": { "constant_value": "us-east-1" }
        }
      }
    },
    "root_module": {
      "resources": [
        { "address": "aws_instance.example", "expressions": {} },
        { "address": "aws_ebs_volume.data1", "expressions": {} },

        { "address": "aws_launch_configuration.lc1", "expressions": {} },
        {
          "address": "aws_autoscaling_group.asg_lc",
          "expressions": {
            "launch_configuration": {
              "references": ["aws_launch_configuration.lc1"]
            }
          }
        },

        { "address": "aws_launch_template.lt1", "expressions": {} },
        {
          "address": "aws_autoscaling_group.asg_lt",
          "expressions": {
            "launch_template": {
              "references": ["aws_launch_template.lt1"]
            }
          }
        }
      ]
    }
  },
  "planned_values": {
    "root_module": {
      "resources": [
        {
          "address": "aws_instance.example",
          "type": "aws_instance",
          "values": {
            "instance_type": "m5.large",
            "tenancy": "default",
            "root_block_device": { "volume_type": "gp2", "size": 100 },
            "ebs_block_device": [
              { "volume_type": "io1", "size": 50, "iops": 3000 },
              { "volume_type": "gp2", "size": 200 }
            ]
          }
        },
        {
          "address": "aws_ebs_volume.data1",
          "type": "aws_ebs_volume",
          "values": { "type": "io1", "size": 200, "iops": 4000 }
          },

       
        {
          "address": "aws_launch_configuration.lc1",
          "type": "aws_launch_configuration",
          "values": {
            "instance_type": "m5.large",
            "placement_tenancy": "default",
            "block_device_mappings": [
              {
                "ebs": [
                  { "volume_type": "io1", "volume_size": 100, "iops": 3000 }
                ]
              },
              {
                "ebs": [
                  { "volume_type": "gp2", "volume_size": 50 }
                ]
              }
            ]
          }
        },
        {
          "address": "aws_autoscaling_group.asg_lc",
          "type": "aws_autoscaling_group",
          "values": {
            "name": "asg-lc-demo"
            
          }
        },

      
        {
          "address": "aws_launch_template.lt1",
          "type": "aws_launch_template",
          "values": {
            "instance_type": "m5.xlarge",
            "block_device_mappings": [
              {
                "ebs": [
                  { "volume_type": "io1", "volume_size": 200, "iops": 4000 }
                ]
              }
            ]
          }
        },
        {
          "address": "aws_autoscaling_group.asg_lt",
          "type": "aws_autoscaling_group",
          "values": {
            "name": "asg-lt-demo",
            "desired_capacity": 3
          }
        }
      ]
    }
  }
}
